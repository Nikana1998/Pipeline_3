pipeline {
    agent any
        stages {
            stage('One') {
                steps {
                    input('Do you want to proceed?')
                }
            }
            stage('Two') {
                steps {
                    echo 'Hello world'
                }
            }
            stage('Three') {
                steps {
                    echo 'This is a declarative pipeline'
                }
            }
        }
}
